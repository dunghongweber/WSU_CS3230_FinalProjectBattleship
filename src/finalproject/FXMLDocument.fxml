<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="myRoot" prefHeight="750.0" prefWidth="950.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="finalproject.FXMLDocumentController">
   <children>
      <VBox alignment="CENTER" prefHeight="750.0" prefWidth="950.0" spacing="30.0">
         <children>
            <Label text="MAIN MENU">
               <font>
                  <Font size="60.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#gotoSceneGOW" prefWidth="230.0" text="Play Game of War">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#gotoSceneBattleship" prefWidth="230.0" text="Play Battleship">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleButtonExit" prefWidth="230.0" text="Exit">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
